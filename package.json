{
  "name": "legalsearch",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --output-path ./dist/angUI",
    "serve-prod": "lite-server --baseDir=dist/angui ",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test-forever": "ng test --watch=true --browsers=ChromeHeadless",
    "test": "ng test --code-coverage",
    "test-prod": "http-server .\\dist\\angUI\\index.html",
    "lint": "ng lint --fix=true",
    "coverage": "ng test --code-coverage",
    "report": "start coverage/legalapprentice/index.html",
    "e2e": "ng e2e",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.html,.ts,.js,.json,.css,.scss}\"",
    "dev": "lite-server --baseDir ./dist/angUI"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.1.0",
    "@angular/material": "~9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@auth0/angular-jwt": "^3.0.1",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "ag-grid-angular": "^22.1.0",
    "ag-grid-community": "^22.1.0",
    "bootstrap": "^4.3.1",
    "d3": "^5.15.0",
    "file-saver": "^2.0.2",
    "gsap": "^3.2.5",
    "ngx-infinite-scroll": "^8.0.2",
    "ngx-toastr": "^12.0.0",
    "rxjs": "~6.5.4",
    "three": "^0.114.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.3",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/three": "^0.103.2",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.0"
  }
